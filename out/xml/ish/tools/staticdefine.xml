<unit xmlns="http://www.srcML.org/srcML/src" xmlns:cpp="http://www.srcML.org/srcML/cpp" revision="1.0.0" language="C" filename="../out/cloned_projects/ish/tools/staticdefine.h"><comment type="line">// credit goes to include/linux/kbuild.h</comment>
<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>_DEFINE</name><parameter_list>(<parameter><type><name>sym</name></type></parameter>, <parameter><type><name>val</name></type></parameter>)</parameter_list></cpp:macro> \
    <cpp:value>asm volatile("\n.ascii \"-&gt;" sym " %0 " #val "\"" : : "i" (val))</cpp:value></cpp:define>
<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>DEFINE</name><parameter_list>(<parameter><type><name>sym</name></type></parameter>, <parameter><type><name>val</name></type></parameter>)</parameter_list></cpp:macro> \
    <cpp:value>_DEFINE(#sym, val)</cpp:value></cpp:define>

<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>BLANK</name><parameter_list>()</parameter_list></cpp:macro> <cpp:value>asm volatile("\n.ascii \"-&gt;\"" : : )</cpp:value></cpp:define>

<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>OFFSET</name><parameter_list>(<parameter><type><name>sym</name></type></parameter>, <parameter><type><name>str</name></type></parameter>, <parameter><type><name>mem</name></type></parameter>)</parameter_list></cpp:macro> \
    <cpp:value>DEFINE(sym##_##mem, offsetof(struct str, mem))</cpp:value></cpp:define>

<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>MACRO</name><parameter_list>(<parameter><type><name>macro</name></type></parameter>)</parameter_list></cpp:macro> \
    <cpp:value>_DEFINE(#macro, macro)</cpp:value></cpp:define>

<cpp:define>#<cpp:directive>define</cpp:directive> <cpp:macro><name>COMMENT</name><parameter_list>(<parameter><type><name>x</name></type></parameter>)</parameter_list></cpp:macro> \
    <cpp:value>asm volatile("\n.ascii \"-&gt;#" x "\"")</cpp:value></cpp:define>
</unit>
